  "author": "Jesse Stuart <hi@jessestuart.com>",
  "dependencies": {
    "apollo-datasource-rest": "0.6.4",
    "apollo-server": "2.9.3",
    "camelcase-keys": "6.0.1",
    "docker-hub-utils": "1.10.6",
    "graphql": "14.5.7",
    "graphql-import": "0.7.1",
    "lodash": "4.17.15",
    "luxon": "1.19.2",
    "pino": "5.13.3",
    "typescript": "3.6.3"
  },
  "devDependencies": {
    "@now/build-utils": "0.10.0",
    "@now/node": "1.0.0",
    "@semantic-release/git": "7.0.16",
    "@types/dotenv": "6.1.1",
    "@types/lodash": "4.14.139",
    "@types/luxon": "1.15.2",
    "@types/node": "12.7.7",
    "@types/pino": "5.8.10",
    "@typescript-eslint/eslint-plugin": "2.3.1",
    "@typescript-eslint/experimental-utils": "2.3.1",
    "@typescript-eslint/parser": "2.3.1",
    "apollo": "2.18.3",
    "apollo-server-testing": "2.9.3",
    "concurrently": "4.1.2",
    "eslint": "6.4.0",
    "eslint-config-prettier": "6.3.0",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-node": "10.0.0",
    "eslint-plugin-prettier": "3.1.1",
    "eslint-plugin-promise": "4.2.1",
    "prettier": "1.18.2",
    "prettier-eslint": "9.0.0",
    "prettier-tslint": "0.4.2",
    "semantic-release": "15.13.24",
    "tslint": "5.20.0",
    "tslint-config-prettier": "1.18.0"
  },
  "keywords": [
    "apollo-server",
    "docker",
    "docker-hub",
    "graphql",
    "typescript"
  ],
  "license": "MIT",
  "main": "api/index.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jessestuart/docker-hub-graphql-api"
  },
  "resolutions": {
    "graphql": "14.5.7"
  },
  "scripts": {
    "check-types": "tsc -p tsconfig.json",
    "lint": "concurrently 'yarn lint:eslint' 'yarn lint:tslint' 'yarn check-types'",
    "lint:eslint": "eslint --cache --ext .ts api",
    "lint:tslint": "tslint -p tsconfig.json",
    "update-schema": "apollo service:push"
  }
