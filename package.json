{
  "name": "docker-hub-graphql-api",
  "version": "0.0.1",
  "author": "Jesse Stuart <hi@jessestuart.com>",
  "description": "A lightweight GraphQL API for querying stats from Docker Hub.",
  "dependencies": {
    "apollo-server": "2.9.1",
    "axios": "0.19.0",
    "docker-hub-utils": "1.2.1",
    "dotenv": "8.1.0",
    "graphql": "14.5.4",
    "graphql-tag": "2.10.1",
    "lodash": "4.17.15",
    "pino": "5.13.2",
    "typescript": "3.6.2"
  },
  "devDependencies": {
    "@now/node": "0.12.7",
    "@types/dotenv": "6.1.1",
    "@types/jest": "24.0.18",
    "@types/lodash": "4.14.138",
    "@types/luxon": "1.15.2",
    "@types/node": "12.7.2",
    "@types/pino": "5.8.9",
    "@typescript-eslint/eslint-plugin": "2.0.0",
    "@typescript-eslint/parser": "2.0.0",
    "concurrently": "4.1.2",
    "eslint": "6.2.2",
    "eslint-config-prettier": "6.1.0",
    "eslint-plugin-graphql": "3.0.3",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-node": "9.2.0",
    "eslint-plugin-prettier": "3.1.0",
    "eslint-plugin-promise": "4.2.1",
    "jest": "24.9.0",
    "jest-junit": "8.0.0",
    "now": "16.1.2",
    "prettier": "1.18.2",
    "prettier-eslint": "9.0.0",
    "prettier-tslint": "0.4.2",
    "ts-jest": "24.0.2",
    "tslint": "5.19.0",
    "tslint-config-prettier": "1.18.0"
  },
  "license": "MIT",
  "main": "api/index.ts",
  "scripts": {
    "check-types": "tsc -p tsconfig.json",
    "lint": "concurrently 'eslint --cache --ext .ts api/' 'yarn tslint -p tsconfig.json'",
    "release": "yarn build && semantic-release",
    "test": "concurrently 'yarn check-types' 'yarn jest --coverage'",
    "test:ci": "jest --runInBand --ci",
    "test:watch": "jest --watch"
  }
}
