    "luxon": "1.19.3",
    "pino": "5.13.5",
    "typescript": "3.6.4"
  },
  "devDependencies": {
    "@now/build-utils": "0.10.1",
    "@now/node": "1.0.1",
    "@semantic-release/git": "7.0.16",
    "@types/dotenv": "6.1.1",
    "@types/lodash": "4.14.144",
    "@types/luxon": "1.15.2",
    "@types/node": "12.11.0",
    "@types/pino": "5.8.12",
    "@typescript-eslint/eslint-plugin": "2.4.0",
    "@typescript-eslint/experimental-utils": "2.4.0",
