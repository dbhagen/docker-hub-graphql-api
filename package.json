{
  "name": "docker-hub-graphql-api",
  "version": "0.0.1",
  "author": "Jesse Stuart <hi@jessestuart.com>",
  "description": "A lightweight GraphQL API for querying stats from Docker Hub.",
  "dependencies": {
    "apollo-server": "2.9.2",
    "axios": "0.19.0",
    "docker-hub-utils": "1.2.2",
    "graphql": "14.5.4",
    "graphql-tag": "2.10.1",
    "graphql-tools": "4.0.5",
    "lodash": "4.17.15",
    "pino": "5.13.2",
    "typescript": "3.6.2"
  },
  "devDependencies": {
    "@now/node": "0.12.7",
    "@types/dotenv": "6.1.1",
    "@types/lodash": "4.14.138",
    "@types/luxon": "1.15.2",
    "@types/node": "12.7.3",
    "@types/pino": "5.8.10",
    "@typescript-eslint/eslint-plugin": "2.0.0",
    "@typescript-eslint/parser": "2.0.0",
    "apollo-server-testing": "2.9.2",
    "concurrently": "4.1.2",
    "eslint": "6.3.0",
    "eslint-config-prettier": "6.1.0",
    "eslint-plugin-graphql": "3.0.3",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-node": "9.2.0",
    "eslint-plugin-prettier": "3.1.0",
    "eslint-plugin-promise": "4.2.1",
    "prettier": "1.18.2",
    "prettier-eslint": "9.0.0",
    "prettier-tslint": "0.4.2",
    "tslint": "5.19.0",
    "tslint-config-prettier": "1.18.0"
  },
  "license": "MIT",
  "main": "api/index.ts",
  "scripts": {
    "check-types": "tsc -p tsconfig.json",
    "lint": "concurrently 'yarn lint:eslint' 'yarn lint:tslint' 'yarn check-types'",
    "lint:eslint": "eslint --cache --ext .ts api",
    "lint:tslint": "tslint -p tsconfig.json",
    "release": "yarn build && semantic-release"
  }
}
