    "prettier-eslint": "9.0.0",
    "prettier-tslint": "0.4.2",
    "semantic-release": "15.13.24",
    "tslint": "5.20.0",
    "tslint-config-prettier": "1.18.0"
  },
  "keywords": [
    "apollo-server",
    "docker",
    "docker-hub",
    "graphql",
    "typescript"
  ],
